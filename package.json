{
  "name": "substratum",
  "version": "0.1.0",
  "description": "Node.js/TypeScript HTTP Server",
  "homepage": "https://github.com/JBreit/substratum#readme",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/JBreit/substratum.git"
  },
  "bugs": {
    "url": "https://github.com/JBreit/substratum/issues"
  },
  "type": "module",
  "main": "./src/apps/substratum/server/start.ts",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "browserslist": "npx browserslist",
    "browserslist:lint": "npx browserslist-lint",
    "build": "cross-env --env-file=.env.development ./node_modules/.bin/tsc --build --verbose",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "clean:coverage": "npx rimraf --preserve-root --glob ./coverage/*",
    "clean:dist": "npx rimraf ./dist",
    "clean:temp": "npx rimraf --preserve-root --glob ./temp/*",
    "commit": "cz",
    "lint": "npx eslint . --cache --max-warnings=0",
    "lint:fix": "npx eslint . --cache --max-warnings=0 --fix",
    "mdlint": "npx markdownlint .",
    "mdlint:fix": "npx markdownlint . --fix",
    "prepare": "npx husky",
    "prettier:editorconfig": "cross-env --env-file=.env.development ./node_modules/.bin/prettier --config ./prettier.config.js --write ./.editorconfig",
    "prettier:format": "cross-env --env-file=.env.development ./node_modules/.bin/prettier --config ./prettier.config.js --write ./src/**/*.ts",
    "prettier:package": "npx prettier-package-json --write",
    "prettier:watch": "cross-env --env-file=.env.development ./node_modules/.bin/onchange ./src/**/*.ts -- ./node_modules/.bin/prettier --config ./prettier.config.js --write {{changed}}",
    "start": "node --env-file=.env.development ./dist/bin/www.js",
    "start:dev": "nodemon",
    "test": "cross-env --env-file=.env.development ./node_modules/.bin/jest --verbose",
    "test:coverage": "npm run clean:coverage && cross-env --env-file=.env.development ./node_modules/.bin/jest --config=jest.config.ts --verbose --coverage",
    "test:watch": "cross-env --env-file=.env.development ./node_modules/.bin/jest --config=jest.config.ts --verbose --watchAll -i",
    "update": "ncu --doctor -u",
    "preversion": "npm run test",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "author": "Jason Breitigan <jason.breitigan@gmail.com>",
  "license": "MIT",
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "dependencies": {
    "awilix": "^12.0.4",
    "awilix-express": "^9.0.2",
    "body-parser": "^1.20.3",
    "chalk": "^5.4.1",
    "compression": "^1.8.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.19.2",
    "express-flash": "^0.0.2",
    "express-mysql-session": "^3.0.3",
    "express-promise-router": "^4.1.1",
    "express-rate-limit": "^7.5.0",
    "express-session": "^1.18.1",
    "express-validator": "^7.2.1",
    "handlebars": "^4.7.8",
    "hbs": "^4.2.0",
    "helmet": "^8.0.0",
    "http-status": "^2.1.0",
    "lodash": "^4.17.21",
    "lusca": "^1.7.0",
    "method-override": "^3.0.0",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.47",
    "mongodb": "^6.14.2",
    "mongoose": "^8.12.1",
    "morgan": "^1.10.0",
    "nodemailer": "^6.10.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "ramda": "^0.30.1",
    "serve-favicon": "^2.5.0",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@commitlint/cz-commitlint": "^19.6.1",
    "@jest/globals": "^29.7.0",
    "@tsconfig/node21": "^21.0.3",
    "@types/bcryptjs": "^2.4.6",
    "@types/bluebird": "^3.5.42",
    "@types/core-js": "^2.5.8",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.1",
    "@types/http-status": "^0.2.30",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.16",
    "@types/lusca": "^1.7.5",
    "@types/method-override": "^3.0.0",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.13.9",
    "@types/nodemailer": "^6.4.17",
    "@types/passport": "^1.0.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/serve-favicon": "^2.5.7",
    "@types/shelljs": "^0.8.15",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.26.0",
    "@typescript-eslint/parser": "^8.26.0",
    "browserslist": "^4.24.4",
    "commitizen": "^4.3.1",
    "conventional-changelog": "^6.0.0",
    "conventional-changelog-angular": "^8.0.0",
    "conventional-changelog-cli": "^5.0.0",
    "cross-env": "^7.0.3",
    "cz-git": "^1.11.1",
    "eslint": "^8.57.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^10.0.2",
    "eslint-config-typescript": "^3.0.0",
    "eslint-import-resolver-typescript": "^3.8.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-watch": "^8.0.0",
    "git-commit-msg-linter": "^5.0.8",
    "git-rev-sync": "^3.0.2",
    "glob": "^11.0.1",
    "glob-all": "^3.3.1",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.4.3",
    "markdownlint": "^0.37.4",
    "markdownlint-cli": "^0.44.0",
    "nodemon": "^3.1.9",
    "onchange": "^7.1.0",
    "prettier": "^3.5.3",
    "prettier-package-json": "^2.8.0",
    "prettier-plugin-ini": "^1.3.0",
    "prettier-plugin-sh": "^0.15.0",
    "prettier-plugin-sql": "^0.18.1",
    "rimraf": "^6.0.1",
    "semver": "^7.7.1",
    "shelljs": "^0.8.5",
    "supertest": "^7.0.0",
    "synk": "^0.0.2",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.11",
    "tsconfig-paths": "^4.2.0",
    "tsconfig-paths-jest": "^0.0.1",
    "typedoc": "^0.27.9",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.0"
  },
  "keywords": [
    "airbnb",
    "async",
    "await",
    "boilerplate",
    "browserslist",
    "commitizen",
    "commitlint",
    "conventional-changelog",
    "cross-env",
    "cz-git",
    "dotenv",
    "editorconfig",
    "eslint",
    "git",
    "husky",
    "jest",
    "lint-staged",
    "markdownlint",
    "node",
    "nodemon",
    "prettier",
    "ts-node",
    "tsconfig-paths",
    "typescript"
  ],
  "engines": {
    "node": "^20.6.0 || ^21.1.0 || ^21.3.0 || >=22.0.0",
    "npm": "^10.5.0 || ^10.6.0 || ^10.7.0 || ^10.8.0 || >=10.8.1"
  }
}
